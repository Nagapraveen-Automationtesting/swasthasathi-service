# Database Configuration
MONGO_CONNECTION_STRING=mongodb://mongo:27017
DATABASE=swasthasathi_db

# Azure Blob Storage Configuration
BLOB_KEY=your_azure_blob_storage_key_here
BLOB_ACCOUNT_NAME=your_azure_storage_account_name
BLOB_CONTAINER=your_blob_container_name

# OCR Service Configuration
OCR_BASE_URL=http://your-ocr-service-url

# JWT Configuration (REQUIRED FOR SECURITY)
JWT_SECRET_KEY=your-super-secret-jwt-key-here-make-it-long-and-random
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=60
JWT_REFRESH_TOKEN_EXPIRE_DAYS=30

# Server Configuration
HOST=0.0.0.0
PORT=8000
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Logging Configuration
LOG_LEVEL=INFO
LOG_TO_FILE=true
LOG_TO_CONSOLE=true
LOG_JSON_FORMAT=false
LOG_FILE_PATH=logs/swasthasathi-service.log

# Debug Mode
DEBUG=false

